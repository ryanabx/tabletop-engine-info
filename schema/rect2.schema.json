{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": ".",
    "title": "Rect2",
    "description": "A rectangle object with position (centered) and size",
    "type": "object",
    "properties": {
        "position": {
            "description": "x-coordinate of this vector",
            "$ref": "./vector2.schema.json"
        },
        "size": {
            "description": "y-coordinate of this vector",
            "$ref": "./vector2.schema.json"
        }
    },
    "required": [ "position", "size" ],
    "additionalProperties": false
}